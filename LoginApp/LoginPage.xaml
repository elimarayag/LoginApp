<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LoginApp.LoginPage"
             Title="LoginPage">
    <Grid BackgroundColor="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="220"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>

        <StackLayout>
            <Frame BackgroundColor="White" HeightRequest="62" WidthRequest="58" CornerRadius="90" Padding="0" Margin="0,100,0,0" HorizontalOptions="Center">
                <Image Source="download.png" Aspect="AspectFill" Margin="0"/>
            </Frame>
        </StackLayout>

    </Grid>
</ContentPage>